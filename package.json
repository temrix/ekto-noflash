{
  "name": "ekto-noflash",
  "version": "2.1.9",
  "description": "Replace the non-free Flash-audioplayer on Ektoplazm.com with a browser-native audioplayer.",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --mode development",
    "build:prod": "webpack",
    "start:dev": "NODE_ENV=development webpack-dev-server --mode=development",
    "web-ext:run": "web-ext run -s dist/",
    "web-ext:build": "web-ext build -s dist/ -a dist/ --overwrite-dest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/temrix/ekto-noflash.git"
  },
  "keywords": [
    "userscript",
    "ektoplazm",
    "audio"
  ],
  "author": "temrix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/temrix/ekto-noflash/issues"
  },
  "homepage": "https://github.com/temrix/ekto-noflash#readme",
  "devDependencies": {
    "buffer": "^6.0.3",
    "lodash": "^4.17.21",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-extension-manifest-plugin": "^0.8.0",
    "webpack-userscript": "^2.5.8"
  }
}
